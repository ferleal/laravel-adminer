{
    "name": "ferleal/laravel-adminer",
    "description": "Adminer (by Jakub Vrana) wrapper for Laravel 5.8+",
    "keywords": ["adminer", "laravel5", "database", "management", "explorer"],
    "license": "MIT",
    "authors": [

        {
            "name": "Fernando Leal",
            "email": "fernandoleal@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "~5.8.0|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Ferleal\\Adminer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ferleal\\Adminer\\AdminerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
